@model POS.Repository.SupplierEntity

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<div class="container">
    <h1></h1>
</div><!-- End Breadcrumbs -->

<section id="contact" class="contact section-bg">
    <div class="container" data-aos="fade-up">
        &nbsp;
        <div class="section-header">
            <h2>Details Suplier</h2>
        </div>
        <div class="container section-bg" style="overflow: auto;">
            <p></p>
            <table class="table table-bordered" style="width:322px">
                <tr>
                    <th>Id</th>
                    <td>@Model.Id</td>
                </tr>
                <tr>
                    <th>Company Name</th>
                    <td>@Model.CompanyName</td>
                </tr>
                <tr>
                    <th>Contact Name</th>
                    <td>@Model.ContactName</td>
                </tr>
                <tr>
                    <th>Contact Title</th>
                    <td>@Model.ContactTitle</td>
                </tr>
                <tr>
                    <th>Address</th>
                    <td>@Model.Address</td>
                </tr>
                <tr>
                    <th>City</th>
                    <td>@Model.City</td>
                </tr>
                <tr>
                    <th>Region</th>
                    <td>@Model.Region</td>
                </tr>
                <tr>
                    <th>Postal Code</th>
                    <td>@Model.PostalCode</td>
                </tr>
                <tr>
                    <th>Country</th>
                    <td>@Model.Country</td>
                </tr>
                <tr>
                    <th>Phone</th>
                    <td>@Model.Phone</td>
                </tr>
                <tr>
                    <th>Fax</th>
                    <td>@Model.Fax</td>
                </tr>
                <tr>
                    <th>Homepage</th>
                    <td>@Model.HomePage</td>
                </tr>
            </table>
        </div>

        <!--End Contact Form -->

    </div>
</section>